citHeader("To cite 'dmetar' in publications use:")

citEntry(entry = "Manual",
  title        = "dmetar: Companion R Package For The Guide 'Doing Meta-Analysis in R'",
  author       = personList(as.person("Mathias Harrer"),
                   as.person("Pim Cuijpers"),
                   as.person("Toshi Furukawa"),
                   as.person("David Daniel Ebert")),
  year         = "2019",
  note         = "R package version 0.0.9000",
  url          = "http://dmetar.protectlab.org",

  textVersion  =
  paste("Harrer, M., Cuijpers, P., Furukawa, T. A. & Ebert, D. D. (2019).",
        "dmetar: Companion R Package For The Guide 'Doing Meta-Analysis in R'.",
        "R package version 0.0.9000. URL http://dmetar.protectlab.org.")
)
